utton\" type=\"primary\" size=\"small\" @click.stop=\"deal(item)\"\r\n            >处理</van-button\r\n          >\r\n          <van-button v-if=\"item.status === 0\" class=\"button\" type=\"warning\" size=\"small\" @click.stop=\"delay(item)\"\r\n            >延期</van-button\r\n          >\r\n        </div>\r\n      </div>\r\n    </van-list>\r\n    <van-dialog v-model=\"showDelay\" title=\"延期申请\" show-cancel-button :beforeClose=\"delaySubmit\">\r\n      <van-form v-model=\"delayItem\">\r\n        <van-field type=\"digit\" v-model=\"delayItem.delay\" name=\"delay\" label=\"延期天数\" placeholder=\"延期天数\" />\r\n        <van-field v-model=\"delayItem.reason\" name=\"uploadremark\" label=\"延期原因\" placeholder=\"延期原因\" />\r\n      </van-form>\r\n    </van-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { getSafeEvent, submitDelaySafeEvent } from '@/api/project.js'\r\nexport default {\r\n  components: {},\r\n  data() {\r\n    return {\r\n      list: [],\r\n      refreshing: false,\r\n      finished: false,\r\n      showDelay: false,\r\n      delayItem: {}\r\n    }\r\n  },\r\n  computed: {\r\n    scrollerHeight: function() {\r\n      return window.innerHeight - 91 + 'px'\r\n    }\r\n  },\r\n  created() {},\r\n  methods: {\r\n    onLoad() {\r\n      let that = this\r\n      getSafeEvent({projectId: this.$store.state.currentBiaoDuan.id}).then(data => {\r\n        let temps = []\r\n        data.data.forEach(item => {\r\n          temps.push({\r\n            ...item,\r\n            ...item.zjSafeEvent\r\n          })\r\n        })\r\n        that.list = temps\r\n        that.refreshing = false\r\n        that.finished = true\r\n      })\r\n    },\r\n    itemClick(item) {\r\n      this.$router.push({\r\n        name: 'securityDetail',\r\n        params: item\r\n      })\r\n    },\r\n    getStateColor(item) {\r\n      return item.delayresult === 2\r\n        ? 'common-state-1'\r\n        : item.delayresult === 3\r\n        ? 'common-state-2'\r\n        : 'common-state-default'\